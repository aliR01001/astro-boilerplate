---
import Layout from '../layouts/Layout.astro';
import { SITE } from '../config/global';

const seo = {
	title: "",
	description: "",
	canonical_url: `${SITE.url}/`,
	ogAlt: "",
	imageUrl: `${SITE.url}/hero-bg.png`,
};

const articleSchema = {
	"@context": "https://schema.org",
	"@graph": [
		{
			"@type": "WebPage",
			"@id": `${seo.canonical_url}/#webpage`,
			url: seo.canonical_url,
			name: seo.title,
			description: seo.description,
			isPartOf: {
				"@id": `${SITE.url}/#website`,
				},
		},
	],
};

---

<Layout {...seo} articleSchema={articleSchema}>

</Layout>
